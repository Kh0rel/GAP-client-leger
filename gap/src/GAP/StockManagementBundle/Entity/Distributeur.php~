<?php

namespace GAP\StockManagementBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Distributeur
 *
 * @ORM\Table()
 * @ORM\Entity(repositoryClass="GAP\StockManagementBundle\Entity\DistributeurRepository")
 */
class Distributeur
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="nomDistrib", type="string", length=255)
     */
    private $nomDistrib;

    /**
     * @var string
     *
     * @ORM\Column(name="adresseDistrib", type="string", length=255)
     */
    private $adresseDistrib;

    /**
     * @var string
     *
     * @ORM\Column(name="cpDistrib", type="string", length=255)
     */
    private $cpDistrib;

    /**
     * @var string
     *
     * @ORM\Column(name="villeDistrib", type="string", length=255)
     */
    private $villeDistrib;

    /**
     * @var string
     *
     * @ORM\Column(name="contactDistrib", type="string", length=255)
     */
    private $contactDistrib;

    /**
     * @var string
     *
     * @ORM\Column(name="mailDistrib", type="string", length=255)
     */
    private $mailDistrib;

    /**
     * @var string
     *
     * @ORM\Column(name="telephoneDistrib", type="string", length=255)
     */
    private $telephoneDistrib;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nomDistrib
     *
     * @param string $nomDistrib
     * @return Distributeur
     */
    public function setNomDistrib($nomDistrib)
    {
        $this->nomDistrib = $nomDistrib;

        return $this;
    }

    /**
     * Get nomDistrib
     *
     * @return string 
     */
    public function getNomDistrib()
    {
        return $this->nomDistrib;
    }

    /**
     * Set adresseDistrib
     *
     * @param string $adresseDistrib
     * @return Distributeur
     */
    public function setAdresseDistrib($adresseDistrib)
    {
        $this->adresseDistrib = $adresseDistrib;

        return $this;
    }

    /**
     * Get adresseDistrib
     *
     * @return string 
     */
    public function getAdresseDistrib()
    {
        return $this->adresseDistrib;
    }

    /**
     * Set cpDistrib
     *
     * @param string $cpDistrib
     * @return Distributeur
     */
    public function setCpDistrib($cpDistrib)
    {
        $this->cpDistrib = $cpDistrib;

        return $this;
    }

    /**
     * Get cpDistrib
     *
     * @return string 
     */
    public function getCpDistrib()
    {
        return $this->cpDistrib;
    }

    /**
     * Set villeDistrib
     *
     * @param string $villeDistrib
     * @return Distributeur
     */
    public function setVilleDistrib($villeDistrib)
    {
        $this->villeDistrib = $villeDistrib;

        return $this;
    }

    /**
     * Get villeDistrib
     *
     * @return string 
     */
    public function getVilleDistrib()
    {
        return $this->villeDistrib;
    }

    /**
     * Set contactDistrib
     *
     * @param string $contactDistrib
     * @return Distributeur
     */
    public function setContactDistrib($contactDistrib)
    {
        $this->contactDistrib = $contactDistrib;

        return $this;
    }

    /**
     * Get contactDistrib
     *
     * @return string 
     */
    public function getContactDistrib()
    {
        return $this->contactDistrib;
    }

    /**
     * Set mailDistrib
     *
     * @param string $mailDistrib
     * @return Distributeur
     */
    public function setMailDistrib($mailDistrib)
    {
        $this->mailDistrib = $mailDistrib;

        return $this;
    }

    /**
     * Get mailDistrib
     *
     * @return string 
     */
    public function getMailDistrib()
    {
        return $this->mailDistrib;
    }

    /**
     * Set telephoneDistrib
     *
     * @param string $telephoneDistrib
     * @return Distributeur
     */
    public function setTelephoneDistrib($telephoneDistrib)
    {
        $this->telephoneDistrib = $telephoneDistrib;

        return $this;
    }

    /**
     * Get telephoneDistrib
     *
     * @return string 
     */
    public function getTelephoneDistrib()
    {
        return $this->telephoneDistrib;
    }
}
